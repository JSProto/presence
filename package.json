{
  "name": "presence",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jake",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^5.11.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "flow-bin": "^0.89.0",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "pre-commit": "^1.2.2",
    "source-map-support": "^0.5.9"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-preset-env": "^1.7.0",
    "iohook": "^0.3.1",
    "robotjs": "^0.5.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "babel": {
    "parser": "babel-eslint",
    "presets": [
      "flow",
      "env"
    ],
    "plugins": [
      [
        "transform-define",
        "./env.config.js"
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "airbnb-base",
      "plugin:flowtype/recommended",
      "prettier"
    ],
    "plugins": [
      "flowtype"
    ],
    "settings": {
      "flowtype": {
        "onlyFilesWithFlowAnnotation": false
      }
    },
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "rules": {
      "flowtype/require-valid-file-annotation": [
        2,
        "always"
      ],
      "no-restricted-syntax": 0,
      "no-console": 0,
      "no-continue": 0,
      "no-plusplus": 0
    }
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": "eslint"
  },
  "scripts": {
    "start": "node index.js",
    "flow": "./node_modules/.bin/flow",
    "lint:js": "eslint ./src",
    "lint-staged": "lint-staged"
  }
}
